#This env stores all shared configs that might be changed later

#A comma separate list of keys that correspond to different config objects
MIDDLEWARE_CONFIG_KEYS=DISCORD,SERVER,SESSION

#SESSION CONFIG
##session.options
SESSION__OPTIONS__NAME=STRING_VALUE
SESSION__OPTIONS__SAVE_UNINITIALIZED=false
SESSION__OPTIONS__RESAVE=false
###session.options.cookie
SESSION__OPTIONS__COOKIE__HTTP_ONLY=true
SESSION__OPTIONS__COOKIE__MAX_AGE=86400000
SESSION__OPTIONS__COOKIE__SAME_SITE=true

#DISCORD CONFIG
##discord.application
DISCORD__APPLICATION__ID=${DISCORD__CLIENT__ID} #Application ID is an alias for Client ID
DISCORD__APPLICATION__PUBLIC_KEY=STRING_VALUE
##discord.bot
DISCORD__BOT__TOKEN=STRING_VALUE
##discord.client
DISCORD__CLIENT__ID=STRING_VALUE
DISCORD__CLIENT__SECRET=STRING_VALUE