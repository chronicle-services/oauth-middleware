#Set environment
NODE_ENV=local

#--CLIENT VARIABLES--#
VITE_AUTH_URL=STRING_VALUE

#--SERVER VARIABLES--#
#SERVER CONFIG
SERVER__HOST__NAME=localhost
SERVER__HOST__URL=${SERVER__MIDDLEWARE__URL}/
##server.middleware
SERVER__MIDDLEWARE__PORT=INT_VALUE
SERVER__MIDDLEWARE__URL=http://localhost:${SERVER__MIDDLEWARE__PORT}/
##server.foundry
SERVER__FOUNDRY__PORT=INT_VALUE
SERVER__FOUNDRY__URL=http://localhost:${SERVER__FOUNDRY__PORT}/
##server.vite
SERVER__VITE__PORT=INT_VALUE
SERVER__VITE__URL=http://localhost:${SERVER__VITE__PORT}/

#DISCORD CONFIG
DISCORD__REDIRECT_URL=${SERVER__MIDDLEWARE__URL}

#--OVERRIDES--#
#SESSION CONFIG
##session.options
SESSION__OPTIONS__SECRET=STRING_VALUE
###sesssion.options.cookie
SESSION__OPTIONS__COOKIE__SECURE=false